{% extends 'IcoderBundle:user:index.html.twig' %}

{% block panelTitle %}
    <h1 class="panel-title col-xs-3">Crear Usuario</h1>
    <a class="btn btn-primary col-xs-3 col-xs-offset-6" href="{{ path('user_index') }}">Listar Usuarios</a>
{% endblock %}
{% block panelBody %}
    <div class="panel-body">
        {{ form_start(form) }}
        {{ form_widget(form) }}
        {{ form_end(form) }}
    </div>
{% endblock %}

{% block panelFooter %}
    <a class="btn btn-primary col-xs-3" href="{{ path('user_index') }}">Listar Usuarios</a>
{% endblock %}
